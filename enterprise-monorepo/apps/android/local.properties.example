# This file contains the local configuration for the Android project
# Copy this file to local.properties and fill in your actual values

# Android SDK location
sdk.dir=/path/to/your/Android/Sdk

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here.apps.googleusercontent.com

# API Configuration
API_BASE_URL=http://10.0.2.2:8080/api/
# For physical device, use your computer's IP:
# API_BASE_URL=http://192.168.1.100:8080/api/

# Keystore for release builds (optional)
# STORE_FILE=/path/to/your/keystore.jks
# STORE_PASSWORD=your_keystore_password
# KEY_ALIAS=your_key_alias
# KEY_PASSWORD=your_key_password